{
    "Logging":  {
        "LogLevel": {
            "Default": "Debug"
        }
    },
    "ConnectionStrings": {
        "Default": "Data Source=Data/MyCourse.old.db"
    },
    "Courses":  {
        "PerPage": 10,
        "Order": {
            "By": "Rating",
            "Ascending": false,
            "Allow": ["Title", "Rating", "CurrentPrice"]
        }
    },
    "ExpirationTime":   {
        "Default": 60
    },
    "MemoryCache": {
        "SizeLimit": 104857600
    }
}